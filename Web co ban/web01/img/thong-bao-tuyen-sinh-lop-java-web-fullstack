<script>
    var score =  2 
    var url = window.location.href;
    $(".share-facebook").attr("href", "https://www.facebook.com/sharer/sharer.php?u=" + url);

    $(".expand-div").click(function () {
        if ($('.post').hasClass("col-lg-9")) {
            $('.post').removeClass('col-lg-9').addClass('col-lg-12')
            $('.post-info').hide()
            $('#header').hide()
            $('#footer').hide()
            $('.breadcrum-div').hide()
            $(this).html('<i class="fas fa-compress" style="color: #5E5E5E;"></i>')
        } else {
            $('.post').removeClass('col-lg-12').addClass('col-lg-9')
            $('.post-info').show()
            $('#header').show()
            $('#footer').show()
            $('.breadcrum-div').show()
            $(this).html('<i class="fas fa-expand" style="color: #5E5E5E;"></i>')
        }
    })

    function LikePost() {
        $.ajax({
            url: "/like-post/35997",
            contentType: "application/json; charset=utf-8",
            type: "POST",
            success: function (data) {
                score++
                $('.vote-score').text("+" + score)
            },
            error: function (result) {
                toastr.error(result.responseJSON.message)
            }
        });
    }

    
    $('.show-modal-login').click(function() {
        $('#modal-signin').modal('show')
    })
    
    $('.cmts-area').focus(function () {
        $(this).next().show(300)
        addAutoResize()
    })

    $('.input-reply-cmt').focus(function () {
            $(this).parent().next().show(300)
            addAutoResize()
        })

    function UpdateContentCmt(btn,e) {
        let content = $(btn).val()
        if (e.keyCode == 13) {
            content = content + "\n";
        }
        return content
    }

    
    $('.btn-send-cmt').click(function () {
        
        
        let content = UpdateContentCmt('.cmts-area', event)

        if(content === '') {
            toastr.error('Bình luận không được để trống.')
            return
        }
        let req = {
            Content: content,
            ParentId : String( 35997 ),
            Level : 1,
            Type : "post"
        }

        var myJSON = JSON.stringify(req);
        var that = this

    $.ajax({
        url: "/comment/create",
        contentType: "application/json; charset=utf-8",
        data: myJSON,
        type: "POST",
        success: function (data) {
            let btnAction = `<div class="cmt-action">
                                <button class="btn-reply">
                                    <span><i class="fas fa-reply"></i></span>
                                    Trả lời
                                </button>
                                <button class="btn-update-cmt" onclick="UpdateItem(this)">Sửa</button>
                                <button class="btn-delete-cmt" onclick="RemoveItem('${data.id}',this)">Xóa</button>
                            </div>
                      
                            <div class="cmt-reply">
                                <div class="list-cmt-reply"></div>
                                <div class="div-hide">
                                    <div class="area-reply">
                                        <img src="(?:)" class="cmt-reply-author-avatar" alt="Avatar">
                                        <textarea class="input-reply-cmt" data-autoresize placeholder="Viết phản hồi"></textarea>
                                    </div>
                                    <div class="comment-author-action">
                                        <button class="btn-send-cmt-reply">Bình luận</button>
                                        <button class="btn-reset-reply">Hủy</button>
                                    </div>
                                </div>
                            </div>`


            let contentHtml =   `<div class="cmt-item" data-item-id="${data.id}">
                                    <div class="comment-author-avatar">
                                        <img src="" alt="Avatar" />
                                    </div>
                                    <div class="comment-author-content">
                                        <span class="name-author-cmt">Phùng Quang Hà</span>
                                        <span class="time-author-cmt">${ConvertTime(data.created_at)}</span>
                                        <pre class="comtent-author-cmt">${data.content}</pre>
                                        ${btnAction}
                                    </div>
                                </div>`

            $(that).parent().parent().parent().parent().parent().find('.list-cmt').append(contentHtml)
            $(that).parent().parent().find('.cmts-area').val("")
            $(that).parent().parent().find('.cmts-area').focus()
            $(that).parent().parent().find('.cmts-area').css({
                height: "4em"
            })
            
        },
        error: function (result) {
            toastr.error(result.responseJSON.message)
        }
    });

    
    })

    
    $('body').on("click", ".btn-send-cmt-reply", function () {
        
        
        let content = $(this).parent().parent().find('.input-reply-cmt').val()
        let parentId = $(this).parent().parent().parent().parent().parent().attr("data-item-id")

        if(content === '') {
            toastr.error('Bình luận không được để trống.')
            return
        }
    
        let req = {
            Content: content,
            ParentId : parentId,
            Level : 2,
            Type : "comment"
        }

        var myJSON = JSON.stringify(req);
        var that = this

    $.ajax({
        url: "/comment/create",
        contentType: "application/json; charset=utf-8",
        data: myJSON,
        type: "POST",
        success: function (data) {
            let btnAction = `<div class="cmt-action">
                                <button class="btn-update-cmt" onclick="UpdateItem(this)">Sửa</button>
                                <button class="btn-delete-cmt" onclick="RemoveItem('${data.id}',this)">Xóa</button>
                            </div>`

            let contentHtml =   `<div class="cmt-item-reply" data-item-id="${data.id}">
                                    <div class="comment-author-avatar">
                                        <img src="(?:)" alt="Avatar" />
                                    </div>
                                    <div class="comment-author-content">
                                        <span class="name-author-cmt">Phùng Quang Hà</span>
                                        <span class="time-author-cmt">${ConvertTime(data.created_at)}</span>
                                        <pre class="comtent-author-cmt">${data.content}</pre>
                                        ${btnAction}
                                    </div>
                                </div>`

            $(that).parent().parent().parent().find('.list-cmt-reply').append(contentHtml)
            $(that).parent().parent().find('.input-reply-cmt').val("")
            $(that).parent().parent().find('.input-reply-cmt').focus()
            $(that).parent().parent().find('.input-reply-cmt').css({
                height: "4em"
            })
        },
        error: function (result) {
            toastr.error(result.responseJSON.message)
        }
    });

    
    })

    
    function checkHasCommentReply(id) {
        let count = $(".post-comments").find(`[data-item-id='${id}']`).find('.cmt-item-reply').length
        return count > 0 ? true : false
    }

    
    function RemoveItem(id,btn) {
        bootbox.confirm({
            message: "Bạn muốn chắc chắn xóa bình luận này không?",
            buttons: {
                confirm: {
                    label: 'Có',
                    className: 'btn-accept'
                },
                cancel: {
                    label: 'Không',
                    className: 'btn-cancel'
                }
            },
            callback: function (result) {
                if(result) {
                    $.ajax({
                        url: '/comment/delete/' + id,
                        contentType: "application/json; charset=utf-8",
                        type: "DELETE",
                        success: function (data) {
                            if(checkHasCommentReply(id)) {
                                messageCommentDeleted = `<p class="comment-is-deleted">
                                                            <i class="fas fa-exclamation-circle"></i>
                                                            Bình luận này đã bị xóa
                                                        <p>`

                                $(btn).parent().parent().find('.comtent-author-cmt:first').remove()
                                $(btn).parent().parent().find('.time-author-cmt:first').after(messageCommentDeleted)
                                $(btn).parent().remove()

                            } else {
                                $('.list-cmt').find(`[data-item-id='${id}']`).remove()
                            }      
                        },
                        error: function (result) {
                            toastr.error(result.responseJSON.message)
                        }
                    });
                } 
            }
        });
    }

    //update comment effect
    function UpdateItem(ele) {
        $('.btn-cancel-cmt').click()
        let idCmt = $(ele).parent().parent().parent().attr('data-item-id')
        let cmtActionAfterUpdate = `<div class="cmt-action-after-update">
                                        <button class="btn-updated-cmt" onclick="UpdateContent('${idCmt}',this)">Cập nhật</button>
                                        <button class="btn-cancel-cmt">Hủy</button>
                                    </div>`
        
        let content = $(ele).parent().parent().find('.comtent-author-cmt:first').text()
        let formUpdateCmt = `<form class="form-update-cmt">
                                <textarea class="input-update-cmt" data-autoresize>${content}</textarea>
                            </form>`
        $(ele).parent().parent().find('.comtent-author-cmt:first').hide()
        $(ele).parent().parent().find('.cmt-action:first').hide()
        $(ele).parent().after(cmtActionAfterUpdate)
        $(ele).parent().before(formUpdateCmt)
    }

    //cancel write comment
    $('body').on("click", ".btn-cancel-cmt", function () {
        $(this).parent().parent().find('.comtent-author-cmt').show()
        $(this).parent().parent().find('.form-update-cmt').remove()
        $(this).parent().parent().find('.cmt-action').show()
        $(this).parent().parent().find('.cmt-action-after-update').remove()
    })

    
    function UpdateContent(id, btn) {
        let content = $(btn).parent().parent().find('.input-update-cmt').val()

        if (content === '') {
            toastr.error('Bình luận không được để trống.')
            return
        }

        let req = {
            Id : id,
            Content: content
        }

        var myJSON = JSON.stringify(req);
        $.ajax({
            url: "/comment/update/" + id,
            contentType: "application/json; charset=utf-8",
            data: myJSON,
            type: "PUT",
            statusCode : {
                200 : function (data) {
                    let messageUpdated = `<span class="message-updated"><i class="fas fa-pencil-alt"></i></span>` 
                    $(btn).parent().parent().find('.comtent-author-cmt').text(data.Content)
                    $(btn).parent().parent().find('.time-author-cmt').text(ConvertTime(data.ModifiedAt))
                    $(btn).parent().parent().find('.time-author-cmt').after(messageUpdated)
                    $(btn).parent().parent().find('.comtent-author-cmt').show()
                    $(btn).parent().parent().find('.form-update-cmt').remove()
                    $(btn).parent().parent().find('.cmt-action').show()
                    $(btn).parent().parent().find('.cmt-action-after-update').remove()
                }
            }
        });
    }


    $('body').on("click", ".btn-reply", function () {
        $(this).parent().parent().find('.div-hide').slideToggle()
    })

    
    function ConvertTime(time) {
        moment.locale('vi')
        let dateTime = moment(time).startOf().fromNow();
        return dateTime
    }

    
    function addAutoResize() {
        document.querySelectorAll('[data-autoresize]').forEach(function (element) {
            element.style.boxSizing = 'border-box';
            var offset = element.offsetHeight - element.clientHeight;
            document.addEventListener('input', function (event) {
                event.target.style.height = 'auto';
                event.target.style.height = event.target.scrollHeight + offset + 'px';
            });
            element.removeAttribute('data-autoresize');
        });
    }

    
    $('body').on("click",".btn-reset", function () {
        $(this).parent().parent().find('.cmts-area').val('')
        $(this).parent().slideUp()
        $(this).parent().parent().find('.cmts-area').css({
            height: "4em"
        })
    })

    $('body').on("click",".btn-reset-reply", function () {
        $(this).parent().parent().find('.input-reply-cmt').val('')
        $(this).parent().parent().slideUp()
        $(this).parent().parent().find('.input-reply-cmt').css({
            height: "4em"
        })
    })

    $(document).ready(function () {
        
        let images = $('.post-content img')
        Array.from(images).map(image => {
            console.log($(image).css('float'));
            if($(image).css('float') == 'left') {
                $(image).css('margin-right', '2rem')
            } else if($(image).css('float') == 'right') {
                $(image).css('margin-left', '2rem')
            } else {
                $(image).css('margin', 'auto')
            }
        })
        
        
       $('#toc').toc({
           'selectors': 'h2,h3',
           'container': '.post-content'
       })

       if($('.post-content').find('h2').length == 0) {
           $('#toc .toc-h3').css("margin-left","0")
       }

       if($('#toc').find('li').length == 0) {
           $('.heading-article').remove()
       }

       
        $("#toc a").on('click', function (event) {
            if (this.hash !== "") {
                event.preventDefault();
                var hash = this.hash;
                
                $('html, body').animate({
                    scrollTop: $(hash).offset().top
                }, 300, function () {
                    window.location.hash = hash;
                });
            }
        });

       
       $('.main-post .post-info').fixedsticky();

       
        $('.time-author-cmt').each(function (index, element) {
            time = $(element).text()
            $(element).text(ConvertTime(time))
        });
    
        let comments =  null 
        let userInfo = {"Authenticated":true,"UserId":"bslsthn0k7qnmh327ij0","UserFullName":"Phùng Quang Hà","UserEmail":"pqha52@gmail.com","UserAvatar":"","Roles":null,"UserPhone":"0383575993","CountCartItem":0,"GithubClientId":"Iv1.cd5b5722dbe9629b","GoogleClientId":"1088344376413-1aveeq4dk7b3tsu0dmof1vh2idv6n3jk.apps.googleusercontent.com","FacebookClientId":"290270177727593","AdminMenu":false}
        
        if(comments && comments.length > 0) {
            
            let notOwnComment = comments.filter((comment, index) => {
                return comment.UserId !== userInfo.UserId
            })
            notOwnComment.map((comment, index) => {
                $('.list-cmt').find(`[data-item-id='${comment.Id}']`).find('.cmt-action .btn-update-cmt:first, .cmt-action .btn-delete-cmt:first').remove()
                $('.list-cmt').find(`[data-item-id='${comment.Id}']`).find('.cmt-action-after-update:first').remove()
            })

            
            let commentsReplyArr = comments.map(comment => comment.ReplyComment)
            commentsReplySingle = [].concat(...commentsReplyArr)
            let notOwnCommentsReplySingle = commentsReplySingle.filter(comment => comment.user_id !== userInfo.UserId && comment.id != '')
            notOwnCommentsReplySingle.map((comment, index) => {
                $('.list-cmt-reply').find(`[data-item-id='${comment.id}']`).find('.cmt-action').remove()
                $('.list-cmt-reply').find(`[data-item-id='${comment.id}']`).find('.cmt-action-after-update').remove()
            })

            
            let commentIsDeleted = comments.filter((comment, index) => {
                return comment.UserId !== userInfo.UserId
            })
        }
        
        
        $('.cmt-reply-author-avatar').attr("src", userInfo.UserAvatar);

        
        if ($('#back-to-top').length) {
            var scrollTrigger = 200, 
                backToTop = function () {
                    var scrollTop = $(window).scrollTop();
                    if (scrollTop > scrollTrigger) {
                        $('#back-to-top').addClass('show');
                    } else {
                        $('#back-to-top').removeClass('show');
                    }
                };
            backToTop();
            $(window).on('scroll', function () {
                backToTop();
            });
            $('#back-to-top').on('click', function (e) {
                e.preventDefault();
                $('html,body').animate({
                    scrollTop: 0
                }, 700);
            });
        }
    })


    
    $('#techmaster-join-jitsi-meeting').click(function(room) {
        let jitsi_room = $(this).data("room");
        if ( true ) {
            var req = {
                IsLogin: true,
                AccessToken: "",
                Room: jitsi_room
            };
            var myJSON = JSON.stringify(req);
            $.ajax({
				url: "/join-jitsi-meeting",
				type: "POST",
				contentType: "application/json; charset=utf-8",
				data: myJSON,
				dataType: "json",
				success: function (data) {
					location.href = data;
				},
				error: function (xhr) {
					toastr.warning(xhr.responseJSON.message)
				}
			});
        } else {
            gapi.load('auth2', function(){
				gapi.auth2.authorize({
					client_id: "1088344376413-1aveeq4dk7b3tsu0dmof1vh2idv6n3jk.apps.googleusercontent.com",
					scope: 'profile email',
					response_type: 'id_token permission'
				}, function (response) {
					if (response.error) {
						toastr.warning("Lỗi không thể đăng nhập Google")
						return;
					}
                    var req = {
                        IsLogin: false,
                        AccessToken: response.access_token,
                        Room: jitsi_room
                    };
                    var myJSON = JSON.stringify(req);
                    $.ajax({
                        url: "/join-jitsi-meeting",
                        type: "POST",
                        contentType: "application/json; charset=utf-8",
                        data: myJSON,
                        dataType: "json",
                        success: function (data) {
                            location.href = data;
                        },
                        error: function (xhr) {
                            toastr.warning(xhr.responseJSON.message)
                        }
                    });
				});
			})
        }
    })
</script>
